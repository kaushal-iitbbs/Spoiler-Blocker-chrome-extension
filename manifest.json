{
  "manifest_version": 2,

  "name": "Hide Avengers Spoilers",
  "description": "Trying to save you from Avengers Spoiler.",
  "version": "1.2",
  "icons": { 
          "96": "icon-avengers.jpg" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.4.0.min.js", "hide-spoiler.js"],
      "run_at": "document_end"
    }
  ]
}
